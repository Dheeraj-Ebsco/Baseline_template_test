locals {
  environment = "{{my_account_type}}"
  workload    = "{{my_new_account}}"
  alias       = "{{my_new_account}}"
  tags        = merge(local.common-tags, { env = "{{my_account_type}}" })

}

module "iam" {
  source                  = "../../modules/iam"
  alias                   = local.alias
  tags                    = local.tags
  allow-manual-parameters = true
  ec2-ssm-role            = true
  cloudability-eid        = "{{my_cloudability_eid}}"
}

module "config" {
  source = "../../modules/config"
  tags   = local.tags
}

module "s3" {
  source = "../../modules/s3"
}